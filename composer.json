{
    "name": "boxhead/asset-use",
    "description": "See what assets are and aren't being used across your site",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "asset use"
    ],
    "support": {
        "docs": "https://github.com/weareboxhead/asset-use/blob/master/README.md",
        "issues": "https://github.com/weareboxhead/asset-use/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Boxhead",
            "homepage": "https://boxhead.io"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "boxhead\\assetuse\\": "src/"
        }
    },
    "extra": {
        "name": "Asset Use",
        "handle": "asset-use",
        "schemaVersion": "1.0.0",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/weareboxhead/asset-use/master/CHANGELOG.md",
        "components": {
            "assetUseService": "boxhead\\assetuse\\services\\AssetUseService"
        },
        "class": "boxhead\\assetuse\\AssetUse"
    }
}
